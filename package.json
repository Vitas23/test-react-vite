{
  "name": "vite-test-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "serve": "vite",
    "build:dev": "tsc && vite build --mode dev",
    "build:prod": "tsc && vite build --mode prod",
    "preview": "vite preview",
    "format:check": "prettier --list-different \"./{src,cypress}/**/*.{js,jsx,ts,tsx,json}\"",
    "format:update": "prettier --write \"./{src,cypress}/**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "eslint \"{src,cypress}/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@fontsource/roboto": "4.5.8",
    "@mui/icons-material": "5.11.0",
    "@mui/lab": "5.0.0-alpha.117",
    "@mui/material": "5.11.6",
    "@types/react-router-dom": "5.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.7.0",
    "simplebar-react": "^3.2.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@iconify/react": "^4.0.1",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "@vitejs/plugin-react": "3.0.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-react": "7.32.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "prettier": "2.8.3",
    "prettier-plugin-organize-imports": "3.2.2",
    "typescript": "4.9.4",
    "vite": "4.0.0"
  }
}
